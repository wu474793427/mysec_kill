<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wuxc.myseckill.mapper.UserMapper" >
    <select id="selectByPrimaryKey" parameterType="java.lang.Long" resultType="com.wuxc.myseckill.entity.User">
        select user_name ,id
        from user
        where id = #{longValue}
    </select>
</mapper>